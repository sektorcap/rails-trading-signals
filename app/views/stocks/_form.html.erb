<%= bootstrap_form_for(@stock, layout: :horizontal, label_col: 'col-sm-2 col-form-label', control_col: 'col-sm-10') do |f| %>

    <%= f.text_field :symbol %>
      <%= f.text_field :name %>
      <%= f.text_field :dividend %>
      <%= f.text_field :market %>
  
  <div class='form-group row'>
    <div class='col-sm-2'></div>
    <div class='col-sm-10'>
      <%= f.submit 'Submit', class: 'btn btn-primary' %>
      <%= back_link class: 'btn btn-default pull-right' %>
    </div>
  </div>

<% end %>
